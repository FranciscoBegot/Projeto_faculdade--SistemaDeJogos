    <function name="slimeWarText" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;#########################################################&quot;" newline="True"/>
            <output expression="&quot;---------------------------------SLIME WAR--------------------------------------&quot;" newline="True"/>
            <output expression="&quot;#########################################################&quot;" newline="True"/>
            <output expression="&quot;&quot;" newline="True"/>
            <output expression="&quot;&quot;" newline="True"/>
            <output expression="&quot;Rules&quot;" newline="True"/>
            <output expression="&quot;&#9679; When a Slime attacks another, it transfers its entire rage level to the attacked Slime.&quot;" newline="True"/>
            <output expression="&quot;&#9679; If a Slime reaches a rage level higher than 5, it disintegrates&quot;" newline="True"/>
            <output expression="&quot;&#9679; The player may choose to split a Slime, transferring part of its rage to a new Slime (respecting the limit of two Slimes per player)&quot;" newline="True"/>
            <output expression="&quot;Lets Start!!&quot;" newline="True"/>
            <declare name="furia1jog1, furia2jog1, furia1jog2, furia2jog2, gosma1, gosma2, escolha" type="Integer" array="False" size=""/>
            <declare name="continuar" type="Boolean" array="False" size=""/>
            <assign variable="furia1jog1" expression="1"/>
            <assign variable="furia2jog1" expression="1"/>
            <assign variable="furia1jog2" expression="1"/>
            <assign variable="furia2jog2" expression="1"/>
            <assign variable="gosma1" expression="2"/>
            <assign variable="gosma2" expression="2"/>
            <while expression="(gosma1&gt; 0) AND (gosma2 &gt; 0)">
                <output expression="&quot;PLAYER 1'S TURN&quot;" newline="True"/>
                <output expression="&quot;Slime Player 1 = &quot; &amp;gosma1" newline="True"/>
                <if expression="furia1jog1 == 0">
                    <then>
                        <output expression="&quot;Slime Fury 2 = &quot;  &amp;furia2jog1" newline="True"/>
                    </then>
                    <else>
                        <if expression="furia2jog1 == 0">
                            <then>
                                <output expression="&quot;Slime Fury 1 = &quot; &amp;furia1jog1" newline="True"/>
                            </then>
                            <else>
                                <output expression="&quot;Slime Fury 1 = &quot; &amp;furia1jog1" newline="True"/>
                                <output expression="&quot;Slime Fury 2 = &quot;  &amp;furia2jog1" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
                <output expression="&quot;Choice: (1) Attack / (2) Split&quot;" newline="True"/>
                <input variable="escolha"/>
                <if expression="escolha == 1">
                    <then>
                        <if expression="furia1jog2 == 0">
                            <then>
                                <output expression="&quot;Attack J2's Slime 2(2)&quot;" newline="True"/>
                            </then>
                            <else>
                                <if expression="furia2jog2 == 0">
                                    <then>
                                        <output expression="&quot;Attack J2 Slime 1(1)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;Attack One of Player 2's Slimes&quot;" newline="True"/>
                                        <output expression="&quot;Attack Slime 1 (1) / Attack Slime 2 (2)&quot;" newline="True"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <input variable="escolha"/>
                        <if expression="escolha == 1">
                            <then>
                                <output expression="&quot;You Have the Fury to Attack&quot;" newline="True"/>
                                <if expression="furia1jog1 == 0">
                                    <then>
                                        <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog1" newline="True"/>
                                        <output expression="&quot;Attack with Fury (2)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="furia2jog1 == 0">
                                            <then>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog1" newline="True"/>
                                                <output expression="&quot;Attack with Fury (1)&quot;" newline="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog1" newline="True"/>
                                                <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog1" newline="True"/>
                                                <output expression="&quot;Attack with Fury 1 (1) or Fury (2)?&quot;" newline="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                                <input variable="escolha"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <assign variable="furia1jog2" expression="furia1jog2 + furia1jog1"/>
                                    </then>
                                    <else>
                                        <assign variable="furia1jog2" expression="furia1jog2 + furia2jog1"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot;You Have the Fury to Attack&quot;" newline="True"/>
                                <if expression="furia1jog1 == 0">
                                    <then>
                                        <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog1" newline="True"/>
                                        <output expression="&quot;Attack with Fury (2)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="furia2jog1 == 0">
                                            <then>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog1" newline="True"/>
                                                <output expression="&quot;Attack with Fury (1)&quot;" newline="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog1" newline="True"/>
                                                <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog1" newline="True"/>
                                                <output expression="&quot;Attack with Fury 1 (1) / Fury (2)?&quot;" newline="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                                <input variable="escolha"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <assign variable="furia2jog2" expression="furia2jog2 + furia1jog1"/>
                                    </then>
                                    <else>
                                        <assign variable="furia2jog2" expression="furia2jog2 + furia2jog1"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <output expression="&quot;Player 1 Attacks! Transferring Fury...&quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;Split Slime Fury 1 to Slime 2? (1)&quot;" newline="True"/>
                        <output expression="&quot;Split Slime Fury 2 to Slime 1? (2)&quot;" newline="True"/>
                        <input variable="escolha"/>
                        <if expression="furia1jog1 &lt; 5 &amp;&amp; furia2jog1 &lt; 5">
                            <then>
                                <output expression="&quot;Slime Fury 1 J1: &quot; &amp;furia1jog1" newline="True"/>
                                <output expression="&quot;Slime Fury 2 J1: &quot; &amp;furia2jog1" newline="True"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <output expression="&quot;How Much Fury Do You Want to Transfer to Slime 2?&quot;" newline="True"/>
                                        <input variable="escolha"/>
                                        <if expression="escolha &gt; furia1jog1 &amp;&amp; escolha &gt; furia2jog1">
                                            <then>
                                                <output expression="&quot;You Don't Have Enough Fury&quot;" newline="True"/>
                                                <do expression="escolha &gt; furia1jog1 &amp;&amp; escolha &gt; furia2jog1">
                                                    <output expression="&quot;How Much Fury Do You Want to Transfer to Slime 2?&quot;" newline="True"/>
                                                    <input variable="escolha"/>
                                                </do>
                                            </then>
                                            <else/>
                                        </if>
                                        <output expression="&quot;Player 1 Divide! Dividing Fury...&quot;" newline="True"/>
                                        <assign variable="furia1jog1" expression="furia1jog1 - escolha"/>
                                        <assign variable="furia2jog1" expression="furia2jog1 + escolha"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;How much fury do you want to transfer to Slime 1?&quot;" newline="True"/>
                                        <input variable="escolha"/>
                                        <if expression="escolha &gt; furia1jog1 &amp;&amp; escolha &gt; furia2jog1">
                                            <then>
                                                <output expression="&quot;You Don't Have Enough Fury&quot;" newline="True"/>
                                                <do expression="escolha &gt; furia1jog1 &amp;&amp; escolha &gt; furia2jog1">
                                                    <output expression="&quot;How much fury do you want to transfer to Slime 1?&quot;" newline="True"/>
                                                    <input variable="escolha"/>
                                                </do>
                                            </then>
                                            <else/>
                                        </if>
                                        <output expression="&quot;Player 1 Divide! Dividing Fury...&quot;" newline="True"/>
                                        <assign variable="furia2jog1" expression="furia2jog1 - escolha"/>
                                        <assign variable="furia1jog1" expression="furia1jog1 + escolha"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot;IMPOSSIBLE TO DIVIDE!!&quot;" newline="True"/>
                                <output expression="&quot;Your Fury is Too High to Share, You Can't Commit Suicide!!&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
                <if expression="furia1jog2 &gt; 5">
                    <then>
                        <output expression="&quot;Player 2 Slime Disintegrated&quot;" newline="True"/>
                        <assign variable="gosma2" expression="gosma2 - 1"/>
                        <assign variable="furia1jog2" expression="0"/>
                    </then>
                    <else>
                        <if expression="furia2jog2 &gt; 5">
                            <then>
                                <output expression="&quot;Player 2 Slime Disintegrated&quot;" newline="True"/>
                                <assign variable="gosma2" expression="gosma2 - 1"/>
                                <assign variable="furia2jog2" expression="0"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
                <if expression="gosma2 = 0">
                    <then>
                        <output expression="&quot;Player 1 Wins!!!&quot;" newline="True"/>
                        <call expression="Novamente"/>
                    </then>
                    <else/>
                </if>
                <output expression="&quot;Player 2's Turn&quot;" newline="True"/>
                <output expression="&quot;Slime Player 2 = &quot; &amp;gosma2" newline="True"/>
                <if expression="furia1jog2 == 0">
                    <then>
                        <output expression="&quot;Slime Fury 2 = &quot;  &amp;furia2jog2" newline="True"/>
                    </then>
                    <else>
                        <if expression="furia2jog2 == 0">
                            <then>
                                <output expression="&quot;Slime Fury 1 = &quot; &amp;furia1jog2" newline="True"/>
                            </then>
                            <else>
                                <output expression="&quot;Slime Fury 1 = &quot; &amp;furia1jog2" newline="True"/>
                                <output expression="&quot;Slime Fury 2 = &quot;  &amp;furia2jog2" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
                <output expression="&quot;Choose: (1) Attack / (2) Divide&quot;" newline="True"/>
                <input variable="escolha"/>
                <if expression="escolha = 1">
                    <then>
                        <if expression="furia1jog1 == 0">
                            <then>
                                <output expression="&quot;Attack J1's Slime 2(2)&quot;" newline="True"/>
                            </then>
                            <else>
                                <if expression="furia2jog2 == 0">
                                    <then>
                                        <output expression="&quot;Attack J1's Slime 1(1)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;Attack One of Player 2's Slimes&quot;" newline="True"/>
                                        <output expression="&quot;Attack Slime 1 (1) / Attack Slime 2 (2)&quot;" newline="True"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <input variable="escolha"/>
                        <if expression="escolha == 1">
                            <then>
                                <output expression="&quot;You Have the Fury to Attack&quot;" newline="True"/>
                                <if expression="furia1jog2 == 0">
                                    <then>
                                        <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog2" newline="True"/>
                                        <output expression="&quot;Attack with Fury (2)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="furia2jog2 == 0">
                                            <then>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog2" newline="True"/>
                                                <output expression="&quot;Attack with Fury (1)&quot;" newline="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog2" newline="True"/>
                                                <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog2" newline="True"/>
                                                <output expression="&quot;Attack with Fury 1(1) / Fury 2 (2) ?&quot;" newline="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                                <input variable="escolha"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <assign variable="furia1jog1" expression="furia1jog1 + furia1jog2"/>
                                    </then>
                                    <else>
                                        <assign variable="furia1jog1" expression="furia1jog1 + furia2jog2"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot;You Have the Fury to Attack&quot;" newline="True"/>
                                <if expression="furia1jog2 == 0">
                                    <then>
                                        <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog2" newline="True"/>
                                        <output expression="&quot;Atacar com Furia (2)&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <if expression="furia2jog2 == 0">
                                            <then>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog2" newline="True"/>
                                                <output expression="&quot;Attack with fury (1)&quot;" newline="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Slime Fury 1 : &quot;&amp;furia1jog2" newline="True"/>
                                                <output expression="&quot;Slime Fury 2 : &quot;&amp;furia2jog2" newline="True"/>
                                                <output expression="&quot;Attack with Fury 1(1) / Fury 2 (2) ?&quot;" newline="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                                <input variable="escolha"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <assign variable="furia2jog1" expression="furia2jog1 + furia1jog2"/>
                                    </then>
                                    <else>
                                        <assign variable="furia2jog1" expression="furia2jog1 + furia2jog2"/>
                                    </else>
                                </if>
                            </else>
                        </if>
                        <output expression="&quot;Player 2 Attacks!! Transferring Fury...&quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;Divide Slime 1's Fury with Slime 2? (1)&quot;" newline="True"/>
                        <output expression="&quot;Divide Slime Fury 2 with Slime 1? (2)&quot;" newline="True"/>
                        <if expression="furia1jog2 &lt; 5 &amp;&amp; furia2jog2 &lt; 5">
                            <then>
                                <output expression="&quot;Slime Fury 1 J2: &quot; &amp;furia1jog2" newline="True"/>
                                <output expression="&quot;Slime Fury 2 J2: &quot; &amp;furia2jog2" newline="True"/>
                                <if expression="escolha == 1">
                                    <then>
                                        <output expression="&quot;How Much Fury Do You Want to Transfer to Goop 2?&quot;" newline="True"/>
                                        <input variable="escolha"/>
                                        <if expression="escolha &gt; furia1jog2 &amp;&amp; escolha &gt; furia2jog2">
                                            <then>
                                                <output expression="&quot;You Don't Have Enough Fury&quot;" newline="True"/>
                                                <do expression="escolha &gt; furia1jog2 &amp;&amp; escolha &gt; furia2jog2">
                                                    <output expression="&quot;How much Fury do you want to transfer to Slime2?&quot;" newline="True"/>
                                                    <input variable="escolha"/>
                                                </do>
                                            </then>
                                            <else/>
                                        </if>
                                        <output expression="&quot;Player 2 Divide! Dividing Fury...&quot;" newline="True"/>
                                        <assign variable="furia1jog2" expression="furia1jog2 - escolha"/>
                                        <assign variable="furia2jog2" expression="furia2jog2 + escolha"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;How much fury do you want to transfer to Slime 1?&quot;" newline="True"/>
                                        <input variable="escolha"/>
                                        <if expression="escolha &gt; furia1jog2 &amp;&amp; escolha &gt; furia2jog2">
                                            <then>
                                                <output expression="&quot;You Don't Have Enough Fury&quot;" newline="True"/>
                                                <do expression="escolha &gt; furia1jog2 &amp;&amp; escolha &gt; furia2jog2">
                                                    <output expression="&quot;How much Fury do you want to transfer to Slime1?&quot;" newline="True"/>
                                                    <input variable="escolha"/>
                                                </do>
                                            </then>
                                            <else/>
                                        </if>
                                        <output expression="&quot;Player 2 Divide! Dividing Fury...&quot;" newline="True"/>
                                        <assign variable="furia2jog2" expression="furia2jog2 - escolha"/>
                                        <assign variable="furia1jog2" expression="furia1jog2 + escolha"/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <output expression="&quot;IMPOSSIBLE TO DIVIDE!!&quot;" newline="True"/>
                                <output expression="&quot;Your Fury is Too High to Share, You Can't Commit Suicide!!&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
                <if expression="furia1jog1 &gt; 5">
                    <then>
                        <output expression="&quot;Player 1's Slime Has Disintegrated&quot;" newline="True"/>
                        <assign variable="gosma1" expression="gosma1 - 1"/>
                        <assign variable="furia1jog1" expression="0"/>
                    </then>
                    <else>
                        <if expression="furia2jog1 &gt; 5">
                            <then>
                                <output expression="&quot;Player 1's Slime Has Disintegrated&quot;" newline="True"/>
                                <assign variable="gosma1" expression="gosma1 - 1"/>
                                <assign variable="furia2jog1" expression="0"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
                <if expression="gosma1 = 0">
                    <then>
                        <output expression="&quot;!!Player 2 Wins!!&quot;" newline="True"/>
                        <call expression="Novamente"/>
                    </then>
                    <else/>
                </if>
            </while>
            <output expression="&quot;Game Over. Play Again? (True) Yes (False) No&quot;" newline="True"/>
            <if expression="continuar  = true">
                <then>
                    <call expression="slimeWarText"/>
                </then>
                <else>
                    <call expression="Main"/>
                </else>
            </if>
            <input variable="continuar"/>
        </body>
    </function>
    <function name="snake" type="None" variable="">
