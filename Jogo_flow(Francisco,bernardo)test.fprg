                                    <call expression="slimeWar"/>
                                </then>
                                <else>
                                    <output expression="&quot;We appreciate your visit&quot;" newline="True"/>
                                    <output expression="&quot;bye :)&quot;" newline="True"/>
                                </else>
                            </if>
