                                </if>
                            </else>
                        </if>
                        <output expression="&quot;Player 2 Attacks!! Transferring Fury...&quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;Split Fury of Slime 1 to Slime 2? (1)&quot;" newline="True"/>
                        <output expression="&quot;Split Fury of Slime 2 to Slime 1? (2)&quot;" newline="True"/>
                        <if expression="furia1jog2 &lt; 5 &amp;&amp; furia2jog2 &lt; 5">
                            <then>
                                <output expression="&quot;Fury Slime 1 J2: &quot; &amp;furia1jog2" newline="True"/>
